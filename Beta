local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()
local Window = OrionLib:MakeWindow({IntroText = "By Midnight Menus",Name = "Rivals Internal BETA", HidePremium = false, SaveConfig = true, ConfigFolder = "MidnightRivals"})

local Tab = Window:MakeTab({
	Name = "Beta / Testing",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

Tab:AddButton({
	Name = "Test 1 ESP",
	Callback = function()
      		print("Aimbot On")
              loadstring(game:HttpGet(('https://raw.githubusercontent.com/angussaw4134/midnightmenus/main/Corneresp')))()
  	end    
})

